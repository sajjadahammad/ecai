{
  "name": "commit-impact-cli",
  "version": "1.0.0",
  "description": "Lists Playwright tests impacted by a git commit (added, removed, modified, including transitive impact from helpers).",
  "main": "dist/cli.js",
  "bin": {
    "commit-impact": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "lint": "eslint src",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "node --test dist/parse-tests.test.js"
  },
  "keywords": ["playwright", "git", "commit", "impact", "tests"],
  "license": "MIT",
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "commander": "^12.1.0",
    "simple-git": "^3.27.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "@types/node": "^22.10.1",
    "eslint": "^9.15.0",
    "prettier": "^3.3.3",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.17.0"
  }
}
